2024-09-19 13:51:49.957955", "modified_by": "Administrator", "docstatus": 0, "idx": 3, "section_title": "Header Button", "route": "header button", "class_name": "elpgsswz", "is_template": 1, "section_group": "Header", "is_full_width": 0, "allow_clone": 0, "is_clone": 0, "section_type": "Static Section", "view_type": "", "device_type": "", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Header Button", "image_option": "Same Document", "allow_update_to_style": 0, "query_by_category": 0, "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "doctype": "Page Section", "content": [], "section_column_css": [], "column_index": 2}, {"name": "PS-24-0000011264", "owner": "Administrator", "creation": "2024-09-13 10:40:42.439513", "modified": "2024-09-19 13:51:49.875002", "modified_by": "Administrator", "docstatus": 0, "idx": 4, "section_title": "Header Form", "route": "header form", "class_name": "klgevjvq", "is_template": 1, "section_group": "Header", "is_full_width": 0, "allow_clone": 0, "is_clone": 0, "section_type": "Form", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Header Form", "image_option": "Same Document", "allow_update_to_style": 0, "query_by_category": 0, "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "<div\n        id=\"contact-modal-bt1\"\n        class=\"hidden fixed inset-0 z-[99] overflow-y-auto bg-gray-600 bg-opacity-50 transition-opacity\"\n      >\n        <div class=\"min-h-screen flex flex-col items-center justify-center\">\n          <div class=\"close-contact-modal-bt1 fixed inset-0\"></div>\n          <div\n            class=\"contact-modal-content w-[90%] max-w-[600px] mx-10 max-h-[1000px] relative my-8\"\n          >\n            <div class=\"close-contact-modal-bt1 flex text-xl justify-end\">\n              <img class=\"cursor-pointer\" src=\"/assets/go1_cms/images/cancel.svg\" alt=\"\" />\n            </div>\n            <div class=\"bg-white rounded-lg shadow-lg p-6 overflow-hidden\">\n              <div class=\"flex justify-center mb-5\">\n                <span class=\"uppercase text-2xl text-[#1877F2] font-bold\"\n                  >{{ title }}</span\n                >\n              </div>\n              {% if form_data %}\n              <form id=\"form-contact-btn1\" class=\"flex flex-col gap-5\">\n                {% for field in form_data.form_fields %} {% if\n                field.field_hidden == 0 %} {% if field.field_type == \"textarea\"\n                %}\n                <div class=\"floating-label\">\n                  <textarea\n                    placeholder=\"{{field.field_placeholder or ''}}\"\n                    rows=\"8\"\n                    class=\"w-full rounded-md px-4 border text-sm pt-2.5\"\n                    name=\"{{ field.field_name }}\"\n                    style=\"resize: none\"\n                    id=\"btn1-{{ field.field_name }}\"\n                    {%\n                    if\n                    field.field_mandatory\n                    %}\n                    required\n                    {%\n                    endif\n                    %}\n                  ></textarea>\n                  <label\n                    for=\"btn1-{{ field.field_name }}\"\n                    class=\"label-textarea text-[#000000B2] text-sm font-normal\"\n                    >{{field.field_label}}</label\n                  >\n                </div>\n                {% else %}\n                <div class=\"floating-label\">\n                  <input\n                    type=\"{{ field.field_type }}\"\n                    placeholder=\"{{field.field_placeholder or ''}}\"\n                    class=\"w-full rounded-md py-4 px-4 border text-sm\"\n                    name=\"{{ field.field_name }}\"\n                    id=\"btn1-{{ field.field_name }}\"\n                    {%\n                    if\n                    field.field_mandatory\n                    %}\n                    required\n                    {%\n                    endif\n                    %}\n                  />\n                  <label\n                    for=\"btn1-{{ field.field_name }}\"\n                    class=\"text-[#000000B2] text-sm font-normal\"\n                    >{{field.field_label}}</label\n                  >\n                </div>\n                {% endif %} {% endif %} {% endfor %}\n                <button\n                  type=\"submit\"\n                  class=\"text-white px-[73.89px] py-[14px] bg-[#1877F2] hover:opacity-95 w-fit mx-auto rounded-xl\"\n                >\n                  {{form_data.btn_text}}\n                </button>\n              </form>\n              {% endif %}\n            </div>\n          </div>\n        </div>\n      </div>", "custom_css": "<style>\n        .floating-label {\n          position: relative;\n        }\n        .floating-label input:focus + label,\n        .floating-label input:not(:placeholder-shown) + label {\n          transform: translateY(-3em);\n          font-size: 0.75rem;\n          color: #4a5568; /* Tailwind's gray-700 */\n          background-color: white;\n        }\n\n        .floating-label textarea:focus + label,\n        .floating-label textarea:not(:placeholder-shown) + label {\n          transform: translateY(-2.2em);\n          font-size: 0.75rem;\n          color: #4a5568; /* Tailwind's gray-700 */\n          background-color: white;\n        }\n\n        .floating-label label {\n          position: absolute;\n          top: 50%;\n          transform: translateY(-50%);\n          left: 10px;\n          pointer-events: none;\n          transition: all 0.2s ease-out;\n          padding: 0 0.25em;\n        }\n\n        .floating-label .label-textarea {\n          top: 10%;\n          transform: translateY(-10%);\n        }\n      </style>", "custom_js": "<script>\n        $(document).ready(function () {\n          $(\".btn-1\").click(() => {\n            $(\"#contact-modal-bt1\").removeClass(\"hidden\");\n            $(\"body\").addClass(\"modal-open\");\n          });\n          $(\".close-contact-modal-bt1\").click(() => {\n            $(\"#contact-modal-bt1\").addClass(\"hidden\");\n            $(\"body\").removeClass(\"modal-open\");\n          });\n        });\n$(document).ready(function () {\n    $(\"#form-contact-btn1\").submit(function (event) {\n      event.preventDefault();\n      let formData = new FormData(this);\n      formData.append(\"utm_source\", getCookie(\"utm_source\") || \"\");\n      formData.append(\"utm_campaign\", getCookie(\"utm_campaign\") || \"\");\n      formData.append(\"source\", getCookie(\"source\") || window.location.href);\n\n      $.ajax({\n        type: \"POST\",\n        url: \"{{form_data.form_action}}\",\n        data: formData,\n        processData: false,\n        contentType: false,\n        headers: {\n            \"X-Frappe-CSRF-Token\": \"{{csrf_token}}\", \n        },\n        success: function (response) {\n          Swal.fire({\n            position: \"bottom-end\",\n            icon: \"success\",\n            title: \"<h4>Gửi thông tin thành công</h4>\",\n            showConfirmButton: false,\n            timer: 1500,\n          });\n          $(\"#form-contact-btn1\")[0].reset();\n          $(\"#contact-modal-bt1\").addClass(\"hidden\");\n          $(\"body\").removeClass(\"modal-open\");\n        },\n        error: function (error) {\n          Swal.fire({\n            position: \"bottom-end\",\n            icon: \"error\",\n            title: \"<h4>Gửi thông tin thất bại</h4>\",\n            showConfirmButton: false,\n            timer: 1500,\n          });\n        },\n      });\n    });\n  });\n      </script>", "form": "SYS-F-DV CSSK Register", "doctype": "Page Section", "content": [{"name": "8aa066b6e1", "owner": "Administrator", "creation": "2024-09-13 10:40:42.439513", "modified": "2024-09-19 13:51:49.875002", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Tiêu đề", "field_key": "title", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Đăng ký tư vấn", "parent": "PS-24-0000011264", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}]