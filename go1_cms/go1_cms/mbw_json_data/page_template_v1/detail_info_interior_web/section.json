ption": "Same Document", "allow_update_to_style": 0, "reference_document": "Mbw Blog Post", "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "image": "/files/Screenshot from 2024-06-03 15-24-05.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "<div\n  class=\"info-detail my-[60px] mx-auto container\"\n>\n        <div\n          class=\"relative text-left owl-carousel\"\n          id=\"blog_related\"\n        >\n</div>\n</div>", "custom_css": "  .text-ellipsis {\n    display: -webkit-box;\n    -webkit-line-clamp: 2;\n    -webkit-box-orient: vertical;\n    overflow: hidden;\n    text-overflow: ellipsis;\n  }\n\n.owl-prev {\n  height: 36px;\n  width: 36px;\n  position: absolute;\n  top: 50%;\n  left: -0.5%;\n  transform: translateY(-50%);\n  background-color: #6B090B !important;\n  color: white !important;\n  border-radius: 100px;\n}\n\n.owl-prev span, .owl-next span {\n  font-size: 20px;\n}\n\n.owl-next {\n  height: 36px;\n  width: 36px;\n  position: absolute;\n  top: 50%;\n  right: -0.5%;\n  transform: translateY(-50%);\n  background-color: #6B090B !important;\n  color: white !important;\n  border-radius: 100px;\n}\n\n.disabled {\n  opacity: 48% !important;\n}\n\n.text-container {\n    display: -webkit-box;\n    -webkit-box-orient: vertical;\n    -webkit-line-clamp: 2;\n    overflow: hidden;\n}", "custom_js": "let jobs = [];\n      const apiRelatedJob =\n        \"/api/method/go1_cms.api.website.blog.get_related_blogs\";\n      const name_blog = localStorage.getItem(\"name_blog\");\n\n      const getRelatedJob = () => {\n        fetch(`${apiRelatedJob}?name=${name_blog}`, {\n          method: \"GET\",\n        })\n          .then((response) => response.json())\n          .then((result) => {\n            console.log(result);\n            if (result.message) {\n              jobs = result.message?.data;\n            }\n            displayJobs();\n          })\n          .catch((error) => {\n            console.log(error);\n          })\n          .finally(() => {\n            console.log(\"Experiment completed\");\n            \n$(\".owl-carousel\").owlCarousel({\n    margin: 38,\n    responsiveClass: true,\n    dots: false,\n    responsive: {\n      0: {\n        items: 1,\n        nav: true,\n      },\n      600: {\n        items: 2,\n        nav: false,\n      },\n      1000: {\n        items: 3,\n        nav: true,\n        loop: false,\n      },\n    },\n  });\n          });\n      };\n\n      function displayJobs() {\n        const jobContainer = document.getElementById(\"blog_related\");\n\n        for (const blog of jobs) {\n          const jobElement = document.createElement(\"a\");\n          jobElement.href = blog.route;\n          jobElement.className =\n            \"w-full h-full item bg-white rounded-lg shadow\";\n          jobElement.innerHTML = `\n            <div class=\"border border-gray-200 rounded-t-lg\">\n              <img\n                class=\"rounded-t-lg h-[247px] w-full\"\n                src=\"${blog.meta_image}\"\n                alt=\"\"\n              />\n            </div>\n            <div class=\"p-5 border border-gray-200 rounded-b-lg min-h-[151px]\">\n              <p class=\"text-gray-500 font-normal text-sm leading-[21px]\">\n                <i class=\"fa fa-clock-o\"></i> ${blog.published_on}\n              </p>\n\n              <a href=\"#\">\n                <h5\n                  class=\"mb-2 text-lg font-bold tracking-tight text-[#212B36]\"\n                >\n                  ${blog.title}\n                </h5>\n              </a>\n              <p\n                class=\"mb-3 font-normal text-sm text-[#637381] dark:text-gray-400 text-container \"\n              >\n                ${blog.blog_intro}\n              </p>\n            </div>\n        `;\n          jobContainer.appendChild(jobElement);\n        }\n      }\n\n      getRelatedJob();", "doctype": "Page Section", "content": [], "section_column_css": [], "column_index": 1}]