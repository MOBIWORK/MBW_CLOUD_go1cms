one": 0, "section_type": "Static Section", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Tại sao nên chọn chúng tôi website tư vấn du học", "image_option": "Same Document", "allow_update_to_style": 0, "query_by_category": 0, "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "image": "/files/Screenshot from 2024-09-18 16-54-37.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "    <div class=\"container mx-auto my-10\">\n      <div class=\"flex xl:flex-row flex-col py-4\">\n        <div class=\"basis-[55%] xl:pr-[25px] p-0 flex flex-col gap-[15px]\">\n          <div class=\"border-l-[6px] border-[#1877F2] pl-5\">\n            <span class=\"font-semibold text-[32px] leading-[38.4px] uppercase\"\n              >{{ title }}</span\n            >\n          </div>\n\n          <span class=\"font-normal text-base text-[#212529] ck-content\"\n            >{{ content }}</span\n          >\n        </div>\n\n        <div class=\"basis-[45%] xl:pl-[25px] p-0\">\n          <div\n            class=\"grid grid-cols-2 gap-4 xl:py-[66.5px] pt-[50px] pb-[60px]\"\n          >\n            <!-- for --> {% for info in infos %}\n            <div class=\"bg-white\">\n              <div class=\"gap-3 pb-[34px]\">\n                <div\n                  id=\"counter\"\n                  class=\"text-[32px] leading-[38.4px] font-medium text-[#004AAD]\"\n                  data-value=\"{{ info.title }}\"\n                >\n                  0\n                </div>\n                <p class=\"font-medium text-lg text-[#637381]\">\n                  {{ info.content }}\n                </p>\n              </div>\n            </div>\n            <!-- endfor -->{% endfor %}\n          </div>\n        </div>\n      </div>\n    </div>", "custom_js": "    <script>\n      const counter = document.querySelectorAll(\"#counter\");\n      counter.forEach((counter) => {\n        const target = counter.getAttribute(\"data-value\");\n        if (target > 1000) {\n          counter.innerText = \"1000+\";\n        } else {\n          counter.innerText = target;\n        }\n      });\n    </script>", "doctype": "Page Section", "content": [{"name": "2ffea775ab", "owner": "Administrator", "creation": "2024-10-09 10:17:44.559132", "modified": "2024-10-09 10:17:44.567037", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Tiêu đề", "field_key": "title", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Vì sao nên chọn MBW EDU", "parent": "PS-24-0000013498", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "e4f0770304", "owner": "Administrator", "creation": "2024-10-09 10:17:44.559132", "modified": "2024-10-09 10:17:44.567037", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "field_label": "Nội dung", "field_key": "content", "field_type": "Long Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Kinh Nghiệm Lâu Năm: MBW Edu đã có hơn 15 năm hoạt động trong lĩnh vực tư vấn du học, với hàng ngàn học sinh, sinh viên đã thành công trên hành trình học tập tại nước ngoài. Kinh nghiệm và sự am hiểu sâu sắc về hệ thống giáo dục quốc tế giúp chúng tôi đưa ra những lời khuyên đúng đắn và kịp thời cho từng học sinh. <br />\nĐội Ngũ Tư Vấn Tận Tâm: MBW Edu tự hào sở hữu đội ngũ chuyên gia tư vấn có chuyên môn cao và đầy nhiệt huyết. Chúng tôi không chỉ tư vấn về chọn trường, ngành học mà còn đồng hành, hỗ trợ học sinh trong suốt quá trình từ nộp hồ sơ đến khi hoàn thành chương trình học tập.<br />\nMạng Lưới Đối Tác Rộng Khắp: Chúng tôi có mối quan hệ đối tác chiến lược với nhiều trường đại học danh tiếng trên toàn thế giới, tạo cơ hội cho học sinh Việt Nam tiếp cận với những chương trình học tập chất lượng cao và học bổng giá trị.<br />\nHỗ Trợ Toàn Diện: MBW Edu cung cấp các dịch vụ hỗ trợ toàn diện từ A đến Z, bao gồm tư vấn chọn trường, chuẩn bị hồ sơ, xin học bổng, hỗ trợ xin visa, và thậm chí cả những hỗ trợ sau khi du học sinh đã đến nước ngoài.", "parent": "PS-24-0000013498", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "b4a6ff62a7", "owner": "Administrator", "creation": "2024-10-09 10:17:44.559132", "modified": "2024-10-09 10:17:44.567037", "modified_by": "Administrator", "docstatus": 0, "idx": 3, "field_label": "Danh sách thông tin", "field_key": "infos", "field_type": "List", "content_type": "Data", "allow_update_to_style": 0, "content": "[{\"title\": 10000, \"content\": \"Sinh vi\\u00ean ghi danh h\\u00e0ng n\\u0103m\", \"idx\": 1}, {\"title\": 20, \"content\": \"\\u0110\\u1ea1i h\\u1ecdc li\\u00ean k\\u1ebft\", \"idx\": 2}, {\"title\": 120, \"content\": \"Ch\\u01b0\\u01a1ng tr\\u00ecnh h\\u1ecdc thu\\u1eadt\", \"idx\": 3}, {\"title\": 15, \"content\": \"C\\u00e1c chuy\\u00ean gia \\u0111\\u01b0\\u1ee3c \\u0111\\u00e0o t\\u1ea1o\", \"idx\": 4}, {\"title\": 10, \"content\": \"Sinh vi\\u00ean ghi danh h\\u00e0ng n\\u0103m\", \"idx\": 5}, {\"title\": 200, \"content\": \"Sinh vi\\u00ean t\\u1ed1t nghi\\u1ec7p\", \"idx\": 6}]", "fields_json": "[{\"field_label\":\"Tiêu đề\",\"field_key\":\"title\",\"field_type\":\"Int\",\"idx\":1},{\"field_label\":\"Nội dung\",\"field_key\":\"content\",\"field_type\":\"Text\",\"idx\":2}]", "parent": "PS-24-0000013498", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}, {"name": "PS-24-0000013499", "owner": "Administrator", "creation": "2024-10-09 10:22:21.397047", "modified": "2024-10-09 10:22:21.407410", "modified_by": "Administrator", "docstatus": 0, "idx": 4, "section_title": "Dịch vụ website tư vấn du học", "route": "d-ch v- website t- v-n du h-c", "class_name": "bosbqkjk", "is_template": 1, "section_group": "Trang tư vấn du học", "is_fu