xt-container {\n        display: -webkit-box;\n        -webkit-box-orient: vertical;\n        -webkit-line-clamp: 2;\n        overflow: hidden;\n      }\n\n      .section {\n        opacity: 0;\n      }\n\n      .section.active {\n        opacity: 1;\n      }", "custom_js": "<script>\n      const jobContainer = document.getElementById(\"jobs-container\");\n      const apiGetAllJob =\n        \"/api/method/go1_cms.api.website.jobs.get_all_job\";\n      const findMore = document.getElementById(\"button-find-more\");\n      const name_section = \"{{ name_section }}\";\n\n      const getAllJob = () => {\n        fetch(`${apiGetAllJob}?name_section=${name_section}`, {\n          method: \"GET\",\n        })\n          .then((res) => res.json())\n          .then((result) => {\n            renderListJob(result.message.data);\n          })\n          .catch((error) => {\n          })\n          .finally(() => {\n          });\n      };\n\n      const renderListJob = (Jobs) => {\n        jobContainer.innerHTML = \"\";\n        Jobs?.slice(0, 6).forEach((job) => {\n          const jobElement = document.createElement(\"div\");\n          jobElement.className =\n            \"text-left w-full p-6 bg-white border border-gray-200 rounded-lg shadow\";\n          jobElement.innerHTML = `\n                  <div class=\"flex item-center justify-between my-3\">\n                    <p\n                      class=\"text-xs py-1 px-5 font-medium text-center text-black border border-gray-500 rounded-lg\"\n                    >\n                      ${job.designation}\n                    </p>\n                    <div class=\"text-gray-500\">${job.pretty_posted_on}</div>\n                  </div>\n                  <div class=\"mt-5\">\n                    <a href=\"/${job.route}\">\n                      <h5\n                        class=\"mb-2 text-2xl font-bold tracking-tight text-black truncate\"\n                      >\n                        ${job.job_title}\n                