    "custom_js": "document.addEventListener('DOMContentLoaded', () => {\n            const tabButtons = document.querySelectorAll('[role=\"tab\"]');\n            const tabPanels = document.querySelectorAll('[role=\"tabpanel\"]');\n\n            tabButtons.forEach(button => {\n                button.addEventListener('click', () => {\n                    // Hide all tab panels\n                    tabPanels.forEach(panel => panel.classList.add('hidden'));\n                    tabPanels.forEach(panel => panel.classList.remove('block'));\n\n                    // Show the selected tab panel\n                    const tabPanel = document.querySelector(button.getAttribute('data-tabs-target'));\n                    tabPanel.classList.remove('hidden');\n                    tabPanel.classList.add('block');\n\n                    // Update the selected state of the tabs\n                    tabButtons.forEach(btn => btn.setAttribute('aria-selected', 'false'));\n                    button.setAttribute('aria-selected', 'true');\n                });\n            });\n        });",
