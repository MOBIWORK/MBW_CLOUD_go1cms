    "web_template": "    <section class=\"w-full discover flex item-center justify-center py-5\">\n      <div class=\"container flex-col text-center\">\n        <div class=\"flex item-center justify-center flex-col\">\n          <p class=\"text-[40px] leading-[48px] font-bold py-3 my-3\">\n            Upload your CV\n          </p>\n          <span class=\"italic font-normal text-base leading-6\"\n            >Make a Difference with Your Online Resume!</span\n          >\n          <input type=\"file\" id=\"fileInput\" style=\"display: none\" />\n        </div>\n        <div class=\"my-6\">\n          <form action=\"\" id=\"upload-cv-form\">\n          <div\n            class=\"grid md:grid-cols-2 items-center gap-16 mx-auto bg-white rounded-md text-[#333] font-[sans-serif]\"\n          >\n            <div\n              class=\"flex items-center justify-center rounded-3xl border border-gray-200 cursor-pointer h-full\"\n              id=\"uploadContainer\"\n            >\n              <div class=\"flex flex-col justify-center items-center md:py-0 py-10 \">\n                <div>\n                  <img\n                    alt=\"\"\n                    src=\"/files/upload.png\"\n                    width=\"48\"\n                    height=\"48\"\n                  />\n                </div>\n                <h1 class=\"font-bold text-xl\">TẢI CV</h1>\n                <p class=\"max-w-sm text-sm\">\n                  Chọn hoặc kéo thả file CV tại đây, Dung lượng file không quá\n                  10M\n                </p>\n                <div class=\"file-name\" id=\"fileName\"></div>\n              </div>\n            </div>\n            <div class=\"space-y-4\">\n              <div class=\"grid xs:grid-cols-1 md:grid-cols-2 gap-3\">\n                <input\n                  type=\"text\"\n                  placeholder=\"Họ Tên\"\n                  class=\"w-full rounded-md py-4 px-4 border text-sm outline-[#007bff]\"\n                  name=\"name\"\n                />\n                <input\n                  type=\"email\"\n                  placeholder=\"Email\"\n                  class=\"w-full rounded-md py-4 px-4 border text-sm outline-[#007bff]\"\n                  name=\"email\"\n                />\n              </div>\n              <input\n                type=\"text\"\n                placeholder=\"Số điện thoại\"\n                class=\"w-full rounded-md py-4 px-4 border text-sm outline-[#007bff]\"\n                name=\"phone-number\"\n              />\n              <select\n                id=\"countries\"\n                class=\"bg-white text-black text-sm rounded-lg border border-blue-500 block w-full p-4\"\n                name=\"country\"\n              >\n                <option selected>Category</option>\n                <option value=\"US\">United States</option>\n                <option value=\"CA\">Canada</option>\n                <option value=\"FR\">France</option>\n                <option value=\"DE\">Germany</option>\n              </select>\n              <textarea\n                placeholder=\"Mô tả hoặc yêu cầu\"\n                rows=\"8\"\n                class=\"w-full rounded-md px-4 border text-sm pt-2.5 outline-[#007bff]\"\n                name=\"description\"\n              ></textarea>\n            </div>\n          </div>\n          <div class=\"w-full flex flex-row justify-center mt-2\">\n            <button\n              type=\"button\"\n              class=\"text-white bg-[#007bff] hover:bg-blue-600 font-semibold rounded-md text-sm px-6 py-3.5 max-w-2xl\"\n              onclick=\"submitForm()\"\n            >\n              Upload your CV\n            </button>\n          </div>\n             </form>\n        </div>\n      </div>\n    </section>\n",
    "custom_js": "  const uploadContainer = document.getElementById(\"uploadContainer\");\n      const fileInput = document.getElementById(\"fileInput\");\n      const fileNameDisplay = document.getElementById(\"fileName\");\n      let fileUpload \n      \n\n      // Show file dialog when clicking the container\n      uploadContainer.addEventListener(\"click\", () => {\n        fileInput.click();\n      });\n\n      // Highlight the drop area when a file is dragged over it\n      uploadContainer.addEventListener(\"dragover\", (e) => {\n        e.preventDefault();\n        uploadContainer.classList.add(\"dragover\");\n      });\n\n      // Remove the highlight when the file is no longer dragged over the drop area\n      uploadContainer.addEventListener(\"dragleave\", () => {\n        uploadContainer.classList.remove(\"dragover\");\n      });\n\n      // Handle the dropped files\n      uploadContainer.addEventListener(\"drop\", (e) => {\n        e.preventDefault();\n        uploadContainer.classList.remove(\"dragover\");\n\n        const files = e.dataTransfer.files;\n        handleFiles(files);\n      });\n\n      // Handle file selection via input\n      fileInput.addEventListener(\"change\", (e) => {\n        const files = e.target.files;\n        handleFiles(files);\n      });\n\n      function handleFiles(files) {\n        if (files.length > 0) {\n          const file = files[0];\n          if (file.size <= 10 * 1024 * 1024) {\n            fileNameDisplay.textContent = `File đã chọn: ${file.name}`;\n            fileUpload = file\n          } else {\n            alert(\"Dung lượng file vượt quá 10MB\");\n            fileNameDisplay.textContent = \"\";\n          }\n        }\n      }\n\n function submitForm() {\n        const form = document.getElementById(\"upload-cv-form\");\n        const formData = new FormData(form);\n\n        const data = {\n          name: formData.get(\"name\"),\n          email: formData.get(\"email\"),\n          phoneNumber: formData.get(\"phone-number\"),\n          country: formData.get(\"country\"),\n          description: formData.get(\"description\"),\n          file: fileUpload,\n        };\n        console.log(data);\n\n         // fetch('your-server-endpoint', {\n        //     method: 'POST',\n        //     body: formData\n        // }).then(response => response.json()).then(result => {\n        //     console.log('Success:', result);\n        // }).catch(error => {\n        //     console.error('Error:', error);\n        // });\n      }",
