\\u1ea3n ph\\u1ea9m \\u0111\\u1ed3 g\\u1ed7 t\\u1ea1i Asura kh\\u00f4ng ch\\u1ec9 l\\u00e0 nh\\u1eefng m\\u00f3n \\u0111\\u1ed3 ti\\u1ec7n \\u00edch m\\u00e0 c\\u00f2n l\\u00e0 nh\\u1eefng t\\u00e1c ph\\u1ea9m ngh\\u1ec7 thu\\u1eadt th\\u00fa v\\u1ecb. T\\u00f4i r\\u1ea5t \\u1ea5n t\\u01b0\\u1ee3ng v\\u1edbi s\\u1ef1 k\\u1ebft h\\u1ee3p gi\\u1eefa t\\u00ednh th\\u1ea9m m\\u1ef9 v\\u00e0 t\\u00ednh ti\\u1ec7n \\u00edch trong t\\u1eebng s\\u1ea3n ph\\u1ea9m m\\u00e0 t\\u00f4i \\u0111\\u00e3 ch\\u1ecdn\\t\\n\", \"info\": \"H\\u1ecdc Vi\\u00ean Kh\\u00f3a Luy\\u1ec7n Thi IELTS\"}, {\"idx\": 4, \"avatar\": \"/files/image-avatar1.png\", \"name\": \"Nguy\\u1ec5n L\\u00ea H\\u1ea3i Phong\\t\", \"content\": \"Asura kh\\u00f4ng ch\\u1ec9 l\\u00e0 m\\u1ed9t c\\u1eeda h\\u00e0ng \\u0111\\u1ed3 g\\u1ed7 th\\u00f4ng th\\u01b0\\u1eddng, m\\u00e0 \\u0111\\u00f3 c\\u00f2n l\\u00e0 m\\u1ed9t \\u0111i\\u1ec3m \\u0111\\u1ebfn tuy\\u1ec7t v\\u1eddi cho nh\\u1eefng ai \\u0111ang t\\u00ecm ki\\u1ebfm nh\\u1eefng s\\u1ea3n ph\\u1ea9m n\\u1ed9i th\\u1ea5t \\u0111\\u1eb9p m\\u1eaft v\\u00e0 ch\\u1ea5t l\\u01b0\\u1ee3ng. T\\u00f4i \\u0111\\u00e3 c\\u00f3 c\\u01a1 h\\u1ed9i tr\\u1ea3i nghi\\u1ec7m v\\u00e0 th\\u1ef1c s\\u1ef1 \\u1ea5n t\\u01b0\\u1ee3ng v\\u1edbi s\\u1ef1 \\u0111a d\\u1ea1ng v\\u00e0 s\\u1ef1 tinh t\\u1ebf trong t\\u1eebng chi ti\\u1ebft c\\u1ee7a c\\u00e1c s\\u1ea3n ph\\u1ea9m t\\u1ea1i \\u0111\\u00e2y\", \"info\": \"H\\u1ecdc Vi\\u00ean Kh\\u00f3a Luy\\u1ec7n Thi IELTS\"}]", "fields_json": "[{\"field_label\":\"Ảnh đại diện\",\"field_key\":\"avatar\",\"field_type\":\"Attach\",\"idx\":1},{\"field_label\":\"Tên\",\"field_key\":\"name\",\"field_type\":\"Text\",\"idx\":2},{\"field_label\":\"nội dung\",\"field_key\":\"content\",\"field_type\":\"Small Text\",\"idx\":3},{\"field_label\":\"thông tin \",\"field_key\":\"info\",\"field_type\":\"Small Text\",\"idx\":4}]", "parent": "PS-24-0000013752", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}, {"name": "PS-24-0000012887", "owner": "Administrator", "creation": "2024-10-07 11:56:49.365805", "modified": "2024-10-07 13:41:22.568627", "modified_by": "Administrator", "docstatus": 0, "idx": 6, "section_title": "Danh sách tin tức trang chủ web dịch vụ vệ sinh", "route": "danh s-ch tin t-c trang ch- web d-ch v- v- sinh", "class_name": "rktskhbp", "is_template": 1, "section_group": "web dịch vụ vệ sinh", "is_full_width": 0, "allow_clone": 1, "is_clone": 0, "section_type": "Custom Section", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Danh sách tin tức trang chủ web dịch vụ vệ sinh", "image_option": "Same Document", "allow_update_to_style": 0, "reference_document": "Mbw Blog Post", "query_by_category": 1, "category": "Tin tức", "no_of_records": 3, "sort_by": "DESC", "dynamic_data": 0, "display_data_randomly": 0, "image": "/files/Screenshot from 2024-09-06 09-34-44.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": " <div class=\"col-span-3 grid grid-cols-1 gap-0 container mx-auto my-10\">\n      <div class=\"flex justify-center items-center h-[50px]\">\n        <span class=\"text-[44px] font-bold text-[#212B36]\">Tin tức</span>\n      </div>\n      <div\n        class=\"list-info text-left grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-10 h-fit mt-[30px]\"\n        id=\"news-container\"\n      ></div>\n    </div>", "custom_css": ".text-container {\n        display: -webkit-box;\n        -webkit-box-orient: vertical;\n        -webkit-line-clamp: 2;\n        overflow: hidden;\n      }\n\n      .text-line {\n        display: -webkit-box;\n        -webkit-box-orient: vertical;\n        -webkit-line-clamp: 1;\n        overflow: hidden;\n      }", "custom_js": "<script>\n      let news = [];\n      let category = \"{{ category }}\"\n      const name_section = \"{{ name_section }}\";\n      const apiBlogList = \"/api/method/go1_cms.api.website.blog.get_blog_list\";\n      const getBlogList = () => {\n        fetch(\n          `${apiBlogList}?name_section=${name_section}\n                  &page_no=1?blog_category=${category}`,\n          {\n            method: \"GET\"\n          }\n        )\n          .then((response) => response.json())\n          .then((result) => {\n            if (result.message) {\n              news = result.message?.data;\n            }\n          })\n          .catch((error) => {\n          })\n          .finally(() => {\n            displayNews();\n          });\n      };\n\n      function displayNews() {\n        const newContainer = document.getElementById(\"news-container\");\n        newContainer.innerHTML = \"\"; // Xóa nội dung cũ\n\n        for (const blog of news.slice(0, 3)) {\n          const blogElement = document.createElement(\"a\");\n          blogElement.href = `/${blog.route}`;\n          blogElement.className =\n            \"w-full h-full item bg-white border border-gray-200 rounded-lg shadow\";\n          blogElement.innerHTML = `\n                      <img\n                          class=\"rounded-t-lg w-full h-[200px] lazy opacity-0\"\n                          data-src=\"${blog.meta_image}\"\n                          alt=\"\"\n                      />\n                      \n                          <div class=\"p-5\">\n                              <div class=\"flex justify-between items-center\">\n                                  <p class=\"text-black font-normal text-sm leading-[21px]\">\n                                    <i class=\"far fa-clock\"></i> ${blog.published_on}\n                                  </p>\n                                  <p class=\"font-normal text-sm leading-7 text-black\">\n                                    <i class=\"fa fa-user-circle\" aria-hidden=\"true\"></i> ${blog.blogger}\n                                  </p>\n    \n                              </div>\n          \n                              <div>\n                                <h5\n                                  class=\"mb-2 text-lg font-bold tracking-tight text-[#212B36]\"\n                                >\n                                  ${blog.title}\n                                </h5>\n                              </div>\n                              <p\n                                class=\"mb-3 font-normal text-sm text-[#637381] dark:text-gray-400 text-container\"\n                              >\n                                ${blog.blog_intro}\n                              </p>\n                          </div>\n                  `;\n          newContainer.appendChild(blogElement);\n        }\n        lazyLoading();\n      }\n\n      getBlogList()\n    </script>", "doctype": "Page Section", "content": [], "section_column_css": [], "column_index": 1}]