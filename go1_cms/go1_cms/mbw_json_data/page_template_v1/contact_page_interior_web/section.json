9 22, \\u1ea5p H\\u1eadu, T\\u00e2n Th\\u00f4ng H\\u1ed9i, C\\u1ee7 Chi, TP.HCM\", \"idx\": 3}]", "fields_json": "[{\"field_label\":\"công ty\",\"field_key\":\"city\",\"field_type\":\"Text\",\"idx\":1},{\"field_label\":\"địa chỉ\",\"field_key\":\"address\",\"field_type\":\"Small Text\",\"idx\":2}]", "parent": "PS-24-0000008518", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}, {"name": "PS-24-0000008519", "owner": "Administrator", "creation": "2024-08-21 16:50:33.847057", "modified": "2024-08-21 16:50:33.847057", "modified_by": "Administrator", "docstatus": 0, "idx": 3, "section_title": "Contact Form Interior Web", "route": "contact form interior web", "class_name": "cpeonjez", "is_template": 0, "section_group": "Features", "is_full_width": 0, "section_type": "Form", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Contact Form Interior Web", "image_option": "Same Document", "allow_update_to_style": 0, "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "image": "/files/Screenshot from 2024-06-03 16-29-34.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "    <div class=\"contact bg-[#FFE1A01F] py-6\">\n      <div class=\"content container mx-auto\">\n        <div class=\"flex justify-center items-center pt-15\">\n          <div><img src=\"/files/kh1.png\" alt=\"\" /></div>\n          <b\n            class=\"md:text-2xl lg:text-[35px] text-[#6b090b] text-nowrap uppercase\"\n            >{{ title }}</b\n          >\n          <div><img src=\"/files/kh1.png\" alt=\"\" /></div>\n        </div>\n        <div\n          class=\"mt-5 flex flex-col justify-center items-center text-base text-[#212B36] font-normal gap-5 max-w-[500px] mx-auto\"\n        >\n          {% for item in descriptions %}\n          <i class=\"text-center\"> {{ item.describe }} </i>\n          {% endfor %}\n        </div>\n      </div>\n      {% if form_data %}\n      <form id=\"form_contact\">\n        <div\n          class=\"mt-10 grid grid-cols-1 gap-x-6 gap-y-6 sm:grid-cols-6 container mx-auto\"\n        >\n          {% for field in form_data.get('form_fields') %} {% if\n          field.field_name == \"first_name\" and field.field_hidden == 0 %}\n          <div class=\"sm:col-span-3\">\n            <div class=\"mt-2\">\n              <input\n                type=\"text\"\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                class=\"bg-[#FFE1A01F] h-[60px] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_label }}\"\n                {% if field.field_mandatory == 1 %} required {% endif %}\n              />\n            </div>\n          </div>\n          {% endif %} {% if field.field_name == \"last_name\" and field.field_hidden == 0 %}\n          <div class=\"sm:col-span-3\">\n            <div class=\"mt-2\">\n              <input\n                type=\"text\"\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                class=\"bg-[#FFE1A01F] h-[60px] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_label }}\"\n                {% if field.field_mandatory == 1 %} required {% endif %}\n              />\n            </div>\n          </div>\n          {% endif %} {% if field.field_name == \"email\" and field.field_hidden == 0 %}\n          <div class=\"sm:col-span-6\">\n            <div class=\"mt-2\">\n              <input\n                type=\"text\"\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                class=\"bg-[#FFE1A01F] h-[60px] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_label }}\"\n                {% if field.field_mandatory == 1 %} required {% endif %}\n              />\n            </div>\n          </div>\n          {% endif %} {% if field.field_name == \"phone_number\" and field.field_hidden == 0 %}\n          <div class=\"sm:col-span-6\">\n            <div class=\"mt-2\">\n              <input\n                type=\"number\"\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                class=\"bg-[#FFE1A01F] h-[60px] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_label }}\"\n                {% if field.field_mandatory == 1 %} required {% endif %}\n              />\n            </div>\n          </div>\n          {% endif %} {% if field.field_name == \"message\" and field.field_hidden == 0 %}\n          <div class=\"col-span-full\">\n            <div class=\"mt-2\">\n              <textarea\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                rows=\"5\"\n                class=\"bg-[#FFE1A01F] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_label }}\"\n                {% if field.field_mandatory == 1 %} required {% endif %}\n              ></textarea>\n            </div>\n          </div>\n          {% endif %} {% endfor %}\n        </div>\n        <div class=\"flex justify-center\">\n          <button type=\"submit\" class=\"mt-4 mb-5\">\n            <div\n              class=\"px-4 py-3 text-white bg-[#6B090B] hover:bg-[#FFE1A0] hover:text-black rounded-lg border border-white hover:border-black cursor-pointer w-[91px] h-[40px] flex justify-center items-center\"\n            >\n              {{ form_data.btn_text }}\n            </div>\n          </button>\n        </div>\n      </form>\n      {% endif %}\n    </div>", "custom_js": "document.getElementById(\"form_contact\").addEventListener(\"submit\", (e) => {\n  e.preventDefault();\n  // Lấy các giá trị từ các trường trong form\n  const formData = {\n    first_name: document.getElementById(\"first_name\").value,\n    last_name: document.getElementById(\"last_name\").value,\n    email: document.getElementById(\"email\").value,\n    phone_number: document.getElementById(\"phone_number\").value,\n    message: document.getElementById(\"message\").value,\n    source: getCookie(\"source\"),\n    utm_source: getCookie(\"utm_source\"),\n    utm_campaign: getCookie(\"utm_campaign\")\n  };\n\n  console.log(formData); // In ra dữ liệu lấy được để kiểm tra\n  fetch(\"/api/method/go1_cms.api.mbw_contact.create_contact\", {\n    method: \"POST\",\n    headers: {\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(formData)\n  })\n    .then((response) => {\n      if (!response.ok) {\n        Swal.fire({\n          icon: \"error\",\n          title: \"Oops...\",\n          text: \"Có lỗi xảy ra, vui lòng thử lại\"\n        });\n        // Kiểm tra nếu mã trạng thái khác 200\n        console.error(`Error: Received status code ${response.status}`);\n        // Bạn có thể thực hiện các hành động khác như hiển thị thông báo lỗi cho người dùng ở đây\n        throw new Error(`HTTP error! status: ${response.status}`);\n      }\n      Swal.fire({\n        title: \"Good job!\",\n        text: \"Bạn đã gửi thông tin thành công\",\n        icon: \"success\"\n      });\n      return response.json();\n    })\n    .then((data) => {\n      console.log(\"Success:\", data);\n      // Bạn có thể thực hiện các hành đ��ng khác như hiển thị thông báo thành công cho người dùng ở đây\n      document.getElementById(\"form_contact\").reset(); // Xóa form khi gửi thành công\n    })\n    .catch((error) => {\n      console.error(\"Error:\", error);\n      Swal.fire({\n        icon: \"error\",\n        title: \"Oops...\",\n        text: \"Có lỗi xảy ra, vui lòng thử lại\"\n      });\n    });\n});\n", "form": "0e0cf2671e", "doctype": "Page Section", "content": [{"name": "ab09bce98d", "owner": "Administrator", "creation": "2024-08-21 16:50:33.847057", "modified": "2024-08-21 16:50:33.847057", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Tiêu đề", "field_key": "title", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Liên hệ", "parent": "PS-24-0000008519", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "a5fb80586d", "owner": "Administrator", "creation": "2024-08-21 16:50:33.847057", "modified": "2024-08-21 16:50:33.847057", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "field_label": "Mô tả", "field_key": "descriptions", "field_type": "List", "content_type": "Data", "allow_update_to_style": 0, "content": "[{\"describe\": \"\\u0110\\u1ec3 l\\u1ea1i th\\u00f4ng tin li\\u00ean h\\u1ec7 ngay v\\u1edbi ch\\u00fang t\\u00f4i\", \"idx\": 1}]", "fields_json": "[{\"field_label\":\"mô tả\",\"field_key\":\"describe\",\"field_type\":\"Small Text\",\"idx\":1}]", "parent": "PS-24-0000008519", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}]