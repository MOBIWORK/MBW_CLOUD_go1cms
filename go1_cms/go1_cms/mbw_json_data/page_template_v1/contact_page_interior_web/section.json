contact interior web", "class_name": "qgokyymi", "is_template": 0, "section_group": "Trang Nội Thất - Interior Web", "is_full_width": 0, "section_type": "Static Section", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Info Contact Interior Web", "image_option": "Same Document", "allow_update_to_style": 0, "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "image": "/files/Screenshot from 2024-06-03 16-22-36.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "        <div\n          class=\"contact-info flex flex-col md:flex-row  2xl:py-[90px] py-5 2xl:gap-[83px] gap-3 mx-auto container\"\n        >\n          <div class=\"info-detail flex-1\">\n            <img src=\"{{ logo_contact }}\" alt=\"Logo\" />\n            <span class=\"font-bold text-[22px]\">{{ company_name }}</span>\n            <div class=\"text-base font-normal flex flex-col mt-6 gap-6\">\n              <div class=\"flex gap-2 items-center\">\n                <img src=\"/files/phone-alert.png\" alt=\"\" class=\"w-4 h-4\"/>\n                <p>{{ phone_number }}</p>\n              </div>\n              <div class=\"flex gap-2 items-center\">\n                <img src=\"/files/mail.png\" alt=\"\" class=\"w-4 h-4\"/>\n                <p>{{ email_support }}</p>\n              </div>\n              <div class=\"flex gap-2 items-center\">\n                <img src=\"/files/outline.png\" alt=\"\" class=\"w-4 h-4\"/>\n                <p>{{ email }}</p>\n              </div>\n            </div>\n            <div class=\"text-base font-normal flex flex-col mt-6 gap-6\">\n              {% for office in offices %}\n              <div class=\"leading-6 text-base flex flex-col italic\">\n                <span class=\"font-bold\">{{ office.city }}</span>\n                <span class=\"font-normal\"\n                  >{{ office.address }}</span\n                >\n              </div>\n             {% endfor %}\n            </div>\n          </div>\n          <div class=\"info-map flex-1 text-center mx-auto flex justify-end\">\n            <img src=\"{{ map }}\" alt=\"map\" />\n          </div>\n        </div>", "doctype": "Page Section", "content": [{"name": "fe72f2d114", "owner": "Administrator", "creation": "2024-08-21 16:50:33.791772", "modified": "2024-08-21 16:50:33.791772", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Logo công ty", "field_key": "logo_contact", "field_type": "Attach", "content_type": "Data", "allow_update_to_style": 0, "content": "/files/Logo-contact.png", "parent": "PS-24-0000008518", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "aecb4893aa", "owner": "Administrator", "creation": "2024-08-21 16:50:33.791772", "modified": "2024-08-21 16:50:33.791772", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "field_label": "Tên công ty", "field_key": "company_name", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Đồ Gỗ ARUSA", "parent": "PS-24-0000008518", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "db3a60dc88", "owner": "Administrator", "creation": "2024-08-21 16:50:33.791772", "modified": "2024-08-21 16:50:33.791772", "modified_by": "Administrator", "docstatus": 0, "idx": 3, "field_label": "Số điện thoại", "field_key": "phone_number", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "02462659519", "parent": "PS-24-0000008518", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "a45d2b095b", "owner": "Administrator", "creation": "2024-08-21 16:50:33.791772", "modified": "2024-08-21 16:50:33.791772", "modified_by": "Administrator", "docstatus": 0, "idx": 4, "field_label": "Email hỗ trợ", "field_key": "email_s