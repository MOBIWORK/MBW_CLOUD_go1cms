{% for field in form_data.form_fields %} {% if field.field_hidden == 0 %}\n      {% if field.field_type == \"text\" or field.field_type == \"email\" %}\n      <div class=\"floating-label\">\n        <input\n          type=\"{{ field.field_type }}\"\n          placeholder=\"{{field.field_placeholder or ''}}\"\n          class=\"w-full rounded-md py-4 px-4 border text-sm\"\n          name=\"{{ field.field_name }}\"\n          id=\"btn1-{{ field.field_name }}\"\n          {%\n          if\n          field.field_mandatory\n          %}\n          required\n          {%\n          endif\n          %}\n        />\n        <label\n          for=\"btn1-{{ field.field_name }}\"\n          class=\"text-[#000000B2] text-sm font-normal\"\n          >{{field.field_label}}</label\n        >\n      </div>\n      {% else %}\n      <div class=\"floating-label\">\n        <textarea\n          placeholder=\"{{field.field_placeholder or ''}}\"\n          rows=\"8\"\n          class=\"w-full rounded-md px-4 border text-sm pt-2.5\"\n          name=\"{{ field.field_name }}\"\n          style=\"resize: none\"\n          id=\"btn1-{{ field.field_name }}\"\n          {%\n          if\n          field.field_mandatory\n          %}\n          required\n          {%\n          endif\n          %}\n        ></textarea>\n        <label\n          for=\"btn1-{{ field.field_name }}\"\n          class=\"label-textarea text-[#000000B2] text-sm font-normal\"\n          >{{field.field_label}}</label\n        >\n      </div>\n      {% endif %} {% endif %} {% endfor %}\n      <button\n        type=\"submit\"\n        class=\"text-white px-[73.89px] py-[14px] bg-[#1877F2] hover:opacity-95 w-fit mx-auto rounded-xl flex\"\n      >\n        {{form_data.btn_text}}\n      </button>\n    </form>\n    {% endif %}\n          </div>\n          <div class=\"col-span-2 pl-5\">\n            <div class=\"flex items-center mt-2 space-x-2 text-black\">\n              <svg\n                xmlns=\"http://www.w3.org/2000/