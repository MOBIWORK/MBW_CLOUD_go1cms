idth": 0, "section_type": "Custom Section", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Tin tức mới nhất Web xây dựng", "image_option": "Same Document", "allow_update_to_style": 0, "reference_document": "Mbw Blog Post", "no_of_records": 4, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "image": "/files/Ảnh chụp màn hình 2024-07-22 141852.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "<div class=\"bg-slate-50 py-[90px]\">\n    <div class=\"container mx-auto xl:pl-[40px] py-[90px]\">\n      <div class=\"flex flex-col gap-[30px]\">\n        <div class=\"flex flex-col gap-[5px] text-center items-center\">\n          <div><span class=\"font-normal text-base leading-6 text-[#FF5E14]\">{{ title }}</span></div>\n          <div><span class=\"font-bold text-[44px] leading-[56px] text-[#231F20] p-2\">{{ tdp }}</span></div>\n        </div>\n \n        <div class=\"\">        \n          <div class=\" grid xl:grid-cols-4 lg:grid-cols-4 md:grid-cols-3 sm:grid-cols-2 gap-[20px] \" id=\"news-container\">\n          \n          </div>\n        </div>\n      </div>\n     </div>\n  </div>", "custom_css": "      .text-container {\n        display: -webkit-box;\n        -webkit-box-orient: vertical;\n        -webkit-line-clamp: 2;\n        overflow: hidden;\n      }", "custom_js": "let news = [];\nconst apiBlogList =\n\"/api/method/go1_cms.api.website.blog.get_blog_list\";\nconst name_section = \"{{ name_section }}\";\n\nconst getBlogList = () => {\n  fetch(\n    `${apiBlogList}?name_section=${name_section}`,\n    {\n      method: \"GET\"\n    }\n  )\n    .then((response) => response.json())\n    .then((result) => {\n      console.log(result);\n      if (result.message) {\n        news = result.message?.data;\n      }\n    })\n    .catch((error) => {\n      console.log(error);\n    })\n    .finally(() => {\n      console.log(\"Call API list new\");\n      displayNews();\n    });\n};\n\nfunction displayNews() {\n  const newContainer = document.getElementById(\"news-container\");\n  newContainer.innerHTML = \"\"; // Xóa nội dung cũ\n\n  for (const blog of news) {\n    const blogElement = document.createElement(\"a\");\n    blogElement.href = `/${blog.route}`;\n    blogElement.className =\n      \"w-full h-full item bg-white border border-gray-200 rounded-lg shadow\";\n    blogElement.innerHTML = `\n            <img\n                class=\"rounded-t-lg w-full\"\n                src=\"https://img.freepik.com/free-photo/painting-mountain-lake-with-mountain-background_188544-9126.jpg\"\n                alt=\"\"\n            />\n            \n                <div class=\"p-5\">\n                    <p class=\"text-gray-500 font-normal text-sm leading-[21px]\">\n                      <i class=\"fa fa-clock-o\"></i> ${blog.published_on} - ${blog.blogger}\n                    </p>\n\n                    <div>\n                      <h5\n                        class=\"mb-2 text-lg font-bold tracking-tight text-[#212B36]\"\n                      >\n                        ${blog.title}\n                      </h5>\n                    </div>\n                    <p\n                      class=\"mb-3 font-normal text-sm text-[#637381] dark:text-gray-400 text-container\"\n                    >\n                      ${blog.blog_intro}\n                    </p>\n                </div>\n        `;\n    newContainer.appendChild(blogElement);\n  }\n}\n\ngetBlogList();\n", "doctype": "Page Section", "content": [{"name": "c3a7bf2c3b", "owner": "Administrator", "creation": "2024-08-29 15:22:04.911525", "modified": "2024-08-29 15:22:04.911525", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "tiêu đề", "field_key": "title", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Tin tức & cập nhật", "parent": "PS-24-0000009995", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "ab843f7f9a", "owner": "Administrator", "creation": "2024-08-29 15:22:04.911525", "modified": "2024-08-29 15:22:04.911525", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "field_label": "tiêu đề phụ", "field_key": "tdp", "field_type": "Long Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Bài viết mới nhất", "parent": "PS-24-0000009995", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}]