r", "docstatus": 0, "idx": 4, "field_label": "Mô tả", "field_key": "des", "field_type": "Long Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Laven Spa là địa chỉ làm đẹp hàng đầu, nơi mang đến cho bạn trải nghiệm thư giãn và chăm sóc sắc đẹp hoàn hảo. Với không gian sang trọng, ấm cúng và dịch vụ chuyên nghiệp, Laven Spa tự hào là điểm đến tin cậy cho những ai yêu thích làm đẹp và chăm sóc sức khỏe.", "parent": "PS-24-0000012777", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "b6659600d9", "owner": "Administrator", "creation": "2024-10-07 09:39:10.667676", "modified": "2024-10-07 09:39:10.678921", "modified_by": "Administrator", "docstatus": 0, "idx": 5, "field_label": "Hình ảnh", "field_key": "img", "field_type": "Attach", "content_type": "Data", "allow_update_to_style": 0, "content": "/files/Image Large9f1e13.png", "parent": "PS-24-0000012777", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}, {"name": "PS-24-0000012779", "owner": "Administrator", "creation": "2024-10-07 09:44:07.474363", "modified": "2024-10-07 09:44:07.482781", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "section_title": "Dịch vụ web spa", "route": "d-ch v- web spa", "class_name": "wqxedymc", "is_template": 1, "section_group": "Trang thẩm mỹ viện spa", "is_full_width": 0, "allow_clone": 1, "is_clone": 0, "section_type": "Static Section", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Dịch vụ web spa", "image_option": "Same Document", "allow_update_to_style": 0, "query_by_category": 0, "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "image": "/files/Screenshot from 2024-07-22 10-13-08.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_tem