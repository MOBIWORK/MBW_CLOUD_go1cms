flex items-center justify-center mt-2\">\n        <div class=\"grid grid-cols-1 xl:grid-cols-5 container xl:gap-10\">\n          <div class=\"col-span-3\">\n               {% if form_data %}\n    <form id=\"form-contact-btn1\" class=\"ml-auto space-y-4\">\n      {% for field in form_data.form_fields %} {% if field.field_hidden == 0 %}\n      {% if field.field_type == \"text\" or field.field_type == \"email\" %}\n      <div class=\"floating-label\">\n        <input\n          type=\"{{ field.field_type }}\"\n          placeholder=\"{{field.field_placeholder or ''}}\"\n          class=\"w-full rounded-md py-4 px-4 border text-sm\"\n          name=\"{{ field.field_name }}\"\n          id=\"btn1-{{ field.field_name }}\"\n          {%\n          if\n          field.field_mandatory\n          %}\n          required\n          {%\n          endif\n          %}\n        />\n        <label\n          for=\"btn1-{{ field.field_name }}\"\n          class=\"text-[#000000B2] text-sm font-normal\"\n          >{{field.field_label}}</label\n        >\n      </div>\n      {% else %}\n      <div class=\"floating-label\">\n        <textarea\n          placeholder=\"{{field.field_placeholder or ''}}\"\n          rows=\"8\"\n          class=\"w-full rounded-md px-4 border text-sm pt-2.5\"\n          name=\"{{ field.field_name }}\"\n          style=\"resize: none\"\n          id=\"btn1-{{ field.field_name }}\"\n          {%\n          if\n          field.field_mandatory\n          %}\n          required\n          {%\n          endif\n          %}\n        ></textarea>\n        <label\n          for=\"btn1-{{ field.field_name }}\"\n          class=\"label-textarea text-[#000000B2] text-sm font-normal\"\n          >{{field.field_label}}</label\n        >\n      </div>\n      {% endif %} {% endif %} {% endfor %}\n      <button\n        type=\"submit\"\n        class=\"text-white px-[73.89px] py-[14px] bg-[#1877F2] hover:opacity-95 w-fit mx-auto rounded-xl flex\"\n      >\n        {{form_data.btn_text}}\n      </button>\n    </form>\n    {% endif %}\n          </div>\n          <div class=\"col-span-2 pl-5\">\n            <div class=\"flex items-center mt-2 space-x-2 text-black\">\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                fill=\"none\"\n                viewBox=\"0 0 24 24\"\n                stroke-width=\"1.5\"\n                stroke=\"currentColor\"\n                aria-hidden=\"true\"\n                class=\"w-4 h-4\"\n              >\n                <path\n                  stroke-linecap=\"round\"\n                  stroke-linejoin=\"round\"\n                  d=\"M2.25 6.75c0 8.284 6.716 15 15 15h2.25a2.25 2.25 0 002.25-2.25v-1.372c0-.516-.351-.966-.852-1.091l-4.423-1.106c-.44-.11-.902.055-1.173.417l-.97 1.293c-.282.376-.769.542-1.21.38a12.035 12.035 0 01-7.143-7.143c-.162-.441.004-.928.38-1.21l1.293-.97c.363-.271.527-.734.417-1.173L6.963 3.102a1.125 1.125 0 00-1.091-.852H4.5A2.25 2.25 0 002.25 4.5v2.25z\"\n                ></path>\n              </svg>\n              <a href=\"tel:11111111111\">{{ phone_number }}</a>\n            </div>\n            <div class=\"flex items-center mt-2 space-x-2 text-black\">\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                fill=\"none\"\n                viewBox=\"0 0 24 24\"\n                stroke-width=\"1.5\"\n                stroke=\"currentColor\"\n                aria-hidden=\"true\"\n                class=\"w-4 h-4\"\n              >\n                <path\n                  stroke-linecap=\"round\"\n                  stroke-linejoin=\"round\"\n                  d=\"M21.75 6.75v10.5a2.25 2.25 0 01-2.25 2.25h-15a2.25 2.25 0 01-2.25-2.25V6.75m19.5 0A2.25 2.25 0 0019.5 4.5h-15a2.25 2.25 0 00-2.25 2.25m19.5 0v.243a2.25 2.25 0 01-1.07 1.916l-7.5 4.615a2.25 2.25 0 01-2.36 0L3.32 8.91a2.25 2.25 0 01-1.07-1.916V6.75\"\n                ></path>\n              </svg>\n              <a href=\"mailto:{{ email_support }}\">{{ email_support }}</a>\n            </div>\n\n            <div class=\"flex items-center mt-2 space-x-2 text-black\">\n             <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n<path d=\"M14 8C14 4.68629 11.3137 2 8 2M14 8C14 11.3137 11.3137 14 8 14M14 8H2M8 2C4.68629 2 2 4.68629 2 8M8 2C8 2 10.6667 4 10.6667 8C10.6667 12 8 14 8 14M8 2C8 2 5.33333 4 5.33333 8C5.33333 12 8 14 8 14M2 8C2 11.3137 4.68629 14 8 14\" stroke=\"#212B36\"/>\n</svg>\n\n              <a href=\"mobiwork.vn\">{{ email }}</a>\n            </div>\n            {% for office in offices %}\n            <div class=\"my-3\">\n              <p class=\"font-bold\">{{ office.name }}</p>\n              <i class=\"\">{{ office.address }}</i>\n            </div>\n            {% endfor %}\n          </div>\n        </div>\n      </div>\n    </section>", "custom_css": " .floating-label {\n        position: relative;\n      }\n      .floating-label input:focus + label,\n      .floating-label input:not(:placeholder-shown) + label {\n        transform: translateY(-3em);\n        font-size: 0.75rem;\n        color: #4a5568; /* Tailwind's gray-700 */\n        background-color: white;\n      }\n\n      .floating-label textarea:focus + label,\n      .floating-label textarea:not(:placeholder-shown) + label {\n        transform: translateY(-2.2em);\n        font-size: 0.75rem;\n        color: #4a5568; /* Tailwind's gray-700 */\n        background-color: white;\n      }\n\n      .floating-label label {\n        position: absolute;\n        top: 50%;\n        transform: translateY(-50%);\n        left: 10px;\n        pointer-events: none;\n        transition: all 0.2s ease-out;\n        padding: 0 0.25em;\n      }\n\n      .floating-label .label-textarea {\n        top: 10%;\n        transform: translateY(-10%);\n      }", "custom_js": "<script>\n      $(document).ready(function () {\n        $(\"#form-contact-btn1\").submit(function (event) {\n          event.preventDefault();\n          let formData = new FormData(this);\n          formData.append(\"utm_source\", getCookie(\"utm_source\") || \"\");\n          formData.append(\"utm_campaign\", getCookie(\"utm_campaign\") || \"\");\n          formData.append(\n            \"source\",\n            getCookie(\"source\") || window.location.href\n          );\n\n          $.ajax({\n            type: \"POST\",\n            url: \"{{form_data.form_action}}\",\n            data: formData,\n            processData: false,\n            contentType: false,\n            headers: {\n              \"X-Frappe-CSRF-Token\": \"{{csrf_token}}\"\n            },\n            success: function (response) {\n              Swal.fire({\n                position: \"bottom-end\",\n                icon: \"success\",\n                title: \"<h4>Gửi thông tin thành công</h4>\",\n                showConfirmButton: false,\n                timer: 1500\n              });\n              $(\"#form-contact-btn1\")[0].reset();\n              $(\"#contact-modal-bt1\").addClass(\"hidden\");\n              $(\"body\").removeClass(\"modal-open\");\n            },\n            error: function (error) {\n              Swal.fire({\n                position: \"bottom-end\",\n                icon: \"error\",\n                title: \"<h4>Gửi thông tin thất bại</h4>\",\n                showConfirmButton: false,\n                timer: 1500\n              });\n            }\n          });\n        });\n      });\n    </script>", "doctype": "Page Section", "content": [{"name": "e10b97c867", "owner": "Administrator", "creation": "2024-08-14 08:24:25.654685", "modified": "2024-09-04 15:29:36.614782", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Thông tin văn phòng ", "field_key": "offices", "field_type": "List", "content_type": "Data", "allow_update_to_style": 0, "content": "[{\"name\":\"Văn phòng tại Hồ Chí Minh\",\"address\":\"290 Cộng Hòa, Phường 13, Quận Tân Bình, TP.HCM\",\"idx\":1},{