 font-weight: 400;\n        font-size: 14px;\n        line-height: 16.8px;\n        color: #637381;\n      }\n\n      .content {\n        font-weight: 400;\n        font-size: 14px;\n        line-height: 16.8px;\n        color: #212b36;\n      }\n.owl-stage .item {\npadding: 10px;\n}\n      img {\n        vertical-align: middle;\n      }\n\n      /* Position the image container (needed to position the left and right arrows) */\n      .container {\n      }\n\n      /* Hide the images by default */\n      .mySlides {\n        height: 500px;\n        position: relative;\n      }\n\n      .mySlides img {\n        height: 500px;\n      }\n\n      /* Add a pointer when hovering over the thumbnail images */\n      .cursor {\n        cursor: pointer;\n      }\n\n      /* Next & previous buttons */\n      .prev,\n      .next {\n        cursor: pointer;\n        position: absolute;\n        top: 50%;\n        transform: translateY(-50%);\n        color: white;\n        font-weight: 400;\n        font-size: 10px;\n        border-radius: 100px;\n        user-select: none;\n        -webkit-user-select: none;\n        background-color: #225771;\n        width: 36px;\n        height: 36px;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        z-index: 9;\n      }\n\n      /* Position the \"next button\" to the right */\n      .next {\n        right: 0;\n      }\n\n      .prev {\n        left: 0;\n      }\n\n      /* On hover, add a black background color with a little bit see-through */\n      .prev:hover,\n      .next:hover {\n        background-color: rgba(0, 0, 0, 0.8);\n      }\n\n      /* Number text (1/3 etc) */\n      .numbertext {\n        color: #f2f2f2;\n        font-size: 12px;\n        padding: 8px 12px;\n        position: absolute;\n        top: 0;\n      }\n\n      /* Container for image text */\n      .caption-container {\n        text-align: center;\n        background-color: #222;\n        padding: 2px 16px;\n        color: white;\n      }\n\n      .row {\n        display: flex;\n      }\n\n      /* Six columns side by side */\n      .column {\n        display: inline-block;\n      }\n\n      .column img {\n        border-radius: 8px;\n      }\n\n      /* Add a transparency effect for thumnbail images */\n      .demo {\n        opacity: 0.6;\n      }\n\n      .activeImage,\n      .demo:hover {\n        opacity: 1;\n        border: 3px solid #225771;\n      }\n\n      .checked {\n        color: orange;\n      }\n\n      .arrow-button {\n        width: 40px;\n        height: 40px;\n        background-color: #000;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        cursor: pointer;\n      }\n\n      .arrow-icon {\n        color: #fff;\n        font-size: 30px;\n      }\n      .floating-label {\n        position: relative;\n      }\n      .floating-label input:focus + label,\n      .floating-label input:not(:placeholder-shown) + label {\n        transform: translateY(-3em);\n        font-size: 0.75rem;\n        color: #4a5568; /* Tailwind's gray-700 */\n        background-color: white;\n      }\n\n      .floating-label textarea:focus + label,\n      .floating-label textarea:not(:placeholder-shown) + label {\n        transform: translateY(-2.2em);\n        font-size: 0.75rem;\n        color: #4a5568; /* Tailwind's gray-700 */\n        background-color: white;\n      }\n\n      .floating-label label {\n        position: absolute;\n        top: 50%;\n        transform: translateY(-50%);\n        left: 10px;\n        pointer-events: none;\n        transition: all 0.2s ease-out;\n        padding: 0 0.25em;\n      }\n\n      .floating-label .label-textarea {\n        top: 10%;\n        transform: translateY(-10%);\n      }", "custom_js": " <script>\n    const apiCreateContact =\n      \"/api/method/go1_cms.api.mbw_contact.create_contact\";\n\n    let slideIndex = 1;\n    let dots = document.getElementsByClassName(\"demo\");\n    let showSlide = document.getElementById(\"show-slide-image\");\n    const leftArrow = document.getElementById(\"left-arrow\");\n    const rightArrow = document.getElementById(\"right-arrow\");\n\n    var owl = $(\"#slide_image\");\n    owl.owlCarousel({\n      items: 4,\n      nav: false,\n      loop: false,\n      autoplay: false,\n      margin: 10,\n      responsive: {\n        0: {\n          items: 1,\n        },\n        425: {\n          items: 3,\n        },\n        768: {\n          items: 4,\n        },\n        1024: {\n          items: 6,\n        },\n        1280: {\n          items: 4,\n        },\n      },\n    });\n\n    $(\"#releated_products\").owlCarousel({\n      items: 1,\n      nav: false,\n      loop: false,\n      autoplay: false,\n      margin: 10,\n      responsive: {\n        0: {\n          items: 1,\n        },\n        768: {\n          items: 2,\n        },\n        1024: {\n          items: 4,\n        },\n        1280: {\n          items: 4,\n        },\n      },\n    });\n\n    $(\".next\").click(function () {\n      owl.trigger(\"next.owl.carousel\");\n    });\n\n    $(\".prev\").click(function () {\n      owl.trigger(\"prev.owl.carousel\", [300]);\n    });\n\n    $(leftArrow).click(function () {\n      owl.trigger(\"prev.owl.carousel\", [300]);\n      if (slideIndex > 1) {\n        slideIndex--;\n        showSlides(slideIndex);\n      }\n    });\n    $(rightArrow).click(function () {\n      owl.trigger(\"next.owl.carousel\");\n      if (slideIndex < dots?.length) {\n        slideIndex++;\n        showSlides(slideIndex);\n      }\n    });\n\n    $(\".hover-slide\").hover(function () {\n      var index = $(this).attr(\"onclick\").match(/\\d+/)[0];\n      currentSlide(parseInt(index, 10));\n    });\n\n    window.addEventListener(\"click\", function (event) {\n      const contactModal = document.getElementById(\"contact-modal\");\n      if (event.target === contactModal) {\n        contactModal.classList.add(\"hidden\");\n      }\n    });\n    // slide shown\n\n    showSlides(slideIndex);\n\n    function currentSlide(n) {\n      showSlides((slideIndex = n));\n    }\n\n    function showSlides(n) {\n      if (dots?.length) {\n        let i;\n        let slide = document.getElementById(\"slide-image\");\n\n        for (i = 0; i < dots.length; i++) {\n          dots[i].className = dots[i].className.replace(\" activeImage\", \"\");\n        }\n        dots[parseInt(slideIndex, 10) - 1].className += \" activeImage\";\n        slide.src = dots[parseInt(slideIndex, 10) - 1].src;\n        showSlide.src = dots[parseInt(slideIndex, 10) - 1].src;\n      }\n    }\n\n\n    // modal preview image\n    const closeModal = document.getElementById(\"close-modal-upload\");\n    document.getElementById(\"slide-image\").addEventListener(\"click\", () => {\n      uploadModal.classList.remove(\"hidden\");\n      $(\"body\").addClass(\"modal-open\");\n    });\n    closeModal.addEventListener(\"click\", () => {\n      uploadModal.classList.add(\"hidden\");\n      $(\"body\").removeClass(\"modal-open\");\n    });\n\n\n    // modal contact\n    const closeModalContact = document.getElementById(\"close-contact-modal\");\n    const contactModal = document.getElementById(\"contact-modal\");\n    const buttonContact = document.getElementById(\"button-contact\");\n    buttonContact.addEventListener(\"click\", () => {\n      contactModal.classList.remove(\"hidden\");\n      $(\"body\").addClass(\"modal-open\");\n    });\n    closeModalContact.addEventListener(\"click\", () => {\n      contactModal.classList.add(\"hidden\");\n      $(\"body\").removeClass(\"modal-open\");\n    });\n\n    $(document).ready(function () {\n      $(\"#form-contact-btn1\").submit(function (event) {\n        event.preventDefault();\n        let formData = new FormData(this);\n        formData.append(\"utm_source\", getCookie(\"utm_source\") || \"\");\n        formData.append(\"utm_campaign\", getCookie(\"utm_campaign\") || \"\");\n        formData.append(\n          \"source\",\n          getCookie(\"source\") || window.location.href\n        );\n\n        $.ajax({\n          type: \"POST\",\n          url: \"{{form_data.form_action}}\",\n          data: formData,\n          processData: false,\n          contentType: false,\n          headers: {\n            \"X-Frappe-CSRF-Token\": \"{{csrf_token}}\",\n          },\n          success: function (response) {\n            Swal.fire({\n              position: \"bottom-end\",\n              icon: \"success\",\n              title: \"<h4>Gửi thông tin thành công</h4>\",\n              showConfirmButton: false,\n              timer: 1500,\n            });\n            $(\"#form-contact-btn1\")[0].reset();\n            $(\"#contact-modal-bt1\").addClass(\"hidden\");\n            $(\"body\").removeClass(\"modal-open\");\n          },\n          error: function (error) {\n            Swal.fire({\n              position: \"bottom-end\",\n              icon: \"error\",\n              title: \"<h4>Gửi thông tin thất bại</h4>\",\n              showConfirmButton: false,\n              timer: 1500,\n            });\n          },\n        });\n      });\n    });\n  </script>", "form": "SYS-F-Construction Contact", "doctype": "Page Section", "content": [{"name": "759ade0d1c", "owner": "Administrator", "creation": "2024-10-10 15:33:19.704994", "modified": "2024-10-16 14:19:07.850422", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Thay thế khi không đặt giá", "field_key": "priceDefault", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Liên hệ", "parent": "PS-24-0000013965", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "69bb9c4c98", "owner": "Administrator", "creation": "2024-10-10 15:33:19.704994", "modified": "2024-10-16 14:19:07.850422", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "field_label": "Tiêu đề thông tin sản phẩm", "field_key": "title_info", "field_type": "Te