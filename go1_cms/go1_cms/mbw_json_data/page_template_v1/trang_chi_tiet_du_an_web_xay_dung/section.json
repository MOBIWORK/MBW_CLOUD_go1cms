ta", "allow_update_to_style": 0, "content": "[{\"menu\": \"Trang ch\\u1ee7\", \"route\": \"/home\", \"idx\": 1}]", "fields_json": "[{\"field_label\":\"Tiêu đề\",\"field_key\":\"menu\",\"field_type\":\"Text\",\"idx\":1},{\"field_label\":\"Đường dẫn\",\"field_key\":\"route\",\"field_type\":\"Text\",\"idx\":2}]", "parent": "PS-24-0000009996", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}, {"name": "PS-24-0000009997", "owner": "Administrator", "creation": "2024-08-29 15:22:21.484171", "modified": "2024-08-29 16:04:59.826837", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "section_title": "Chi tiết dự án web xây dựng", "route": "chi ti-t d- -n web x-y d-ng", "class_name": "omjsserh", "is_template": 0, "section_group": "web xây dựng", "is_full_width": 0, "section_type": "Custom Section", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Chi tiết dự án web xây dựng", "image_option": "Same Document", "allow_update_to_style": 0, "reference_document": "Mbw Blog Post", "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "   <div class=\"container mx-auto\">\n      <section class=\"flex flex-col gap-[30px]\">\n        <span class=\"span-black text-[44px] font-bold\"\n          >{{ blog_detail.title }}</span\n        >\n        <div>\n         {{ blog_detail.content }}\n        </div>\n      </section>\n    </div>\n\n    <section class=\"w-full discover flex item-center justify-center py-5\">\n      <div class=\"container flex-col text-center\">\n        <div class=\"flex item-center justify-start flex-col gap-5\">\n          <p class=\"text-5xl font-bold\">{{ title }}</p>\n        </div>\n        <div\n          class=\"text-left grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-10 mt-[38px]\"\n          id=\"blog_related\"\n        ></div>\n      </div>\n    </section>", "custom_js": "let jobs = [];\n      const apiRelatedJob =\n        \"/api/method/go1_cms.api.website.blog.get_related_blogs\";\n      const name_blog = \"{{ blog_detail.name }}\";\n\n      const getRelatedJob = () => {\n        fetch(`${apiRelatedJob}?name=${name_blog}`, {\n          method: \"GET\",\n        })\n          .then((response) => response.json())\n          .then((result) => {\n            console.log(result);\n            if (result.message) {\n              jobs = result.message?.data;\n            }\n          })\n          .catch((error) => {\n            console.log(error);\n          })\n          .finally(() => {\n            console.log(\"Experiment completed\");\n            displayJobs();\n          });\n      };\n\n      function displayJobs() {\n        const jobContainer = document.getElementById(\"blog_related\");\n\n        for (const blog of jobs) {\n          const jobElement = document.createElement(\"a\");\n          jobElement.href = `/${blog.route}`;\n          jobElement.className =\n            \"w-full h-full item bg-white border border-gray-200 rounded-lg shadow\";\n          jobElement.innerHTML = `\n            <div>\n              <img\n                class=\"rounded-t-lg h-[247px] w-full\"\n                src=\"${blog.meta_image}\"\n                alt=\"\"\n              />\n            </div>\n            <div class=\"p-5\">\n              <p class=\"text-gray-500 font-normal text-sm leading-[21px]\">\n                <i class=\"fa fa-clock-o\"></i> ${blog.published_on}\n              </p>\n\n              <a href=\"#\">\n                <h5\n                  class=\"mb-2 text-lg font-bold tracking-tight text-[#212B36]\"\n                >\n                  ${blog.title}\n                </h5>\n              </a>\n              <p\n                class=\"mb-3 font-normal text-sm text-[#637381] dark:text-gray-400 text-container\"\n              >\n                ${blog.blog_intro}\n              </p>\n            </div>\n        `;\n          jobContainer.appendChild(jobElement);\n        }\n      }\n\n      getRelatedJob();", "doctype": "Page Section", "content": [{"name": "68925d6e46", "owner": "Administrator", "creation": "2024-08-29 15:22:21.484171", "modified": "2024-08-29 16:04:59.826837", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Tiêu đề", "field_key": "title", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Tin liên quan", "parent": "PS-24-0000009997", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}], "section_column_css": [], "column_index": 1}]