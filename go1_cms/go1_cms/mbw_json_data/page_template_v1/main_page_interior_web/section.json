", "owner": "Administrator", "creation": "2024-10-12 10:13:47.256822", "modified": "2024-10-12 10:13:59.583485", "modified_by": "Administrator", "docstatus": 0, "idx": 9, "section_title": "Contact Form Interior Web", "route": "contact form interior web", "class_name": "btumzkpl", "is_template": 1, "section_group": "Trang Nội Thất - Interior Web", "is_full_width": 0, "allow_clone": 1, "is_clone": 0, "section_type": "Form", "view_type": "", "device_type": "Web & Mobile", "is_editable": 0, "is_login_required": 0, "content_type": "", "fetch_product": 0, "section_name": "Contact Form Interior Web", "image_option": "Same Document", "allow_update_to_style": 0, "query_by_category": 0, "no_of_records": 0, "sort_by": "DESC", "dynamic_data": 1, "display_data_randomly": 0, "image": "/files/Screenshot from 2024-06-03 16-29-34.png", "enable_view_more": 0, "custom_section_data": "[]", "bg_type": "Color", "bg_color": "#ffffff", "web_template": "    <div class=\"contact bg-[#FFE1A01F] py-6 my-10\">\n      <div class=\"content container mx-auto\">\n        <div class=\"flex justify-center items-center pt-15\">\n          <div><img src=\"/files/kh1.png\" alt=\"\" /></div>\n          <b\n            class=\"md:text-2xl lg:text-[35px] text-[#6b090b] text-nowrap uppercase\"\n            >{{ title }}</b\n          >\n          <div><img src=\"/files/kh1.png\" alt=\"\" /></div>\n        </div>\n        <div\n          class=\"mt-5 flex flex-col justify-center items-center text-base text-[#212B36] font-normal gap-5 max-w-[500px] mx-auto\"\n        >\n          {% for item in descriptions %}\n          <i class=\"text-center\"> {{ item.describe }} </i>\n          {% endfor %}\n        </div>\n      </div>\n         {% if form_data %}\n      <form id=\"form-contact-btn1\">\n        <div\n          class=\"mt-10 grid grid-cols-1 gap-x-6 gap-y-6 sm:grid-cols-6 container mx-auto\"\n        >\n          {% for field in form_data.get('form_fields') %} {% if field.field_hidden == 0 %}\n          {% if\n          field.field_name == \"first_name\" %}\n          <div class=\"sm:col-span-3\">\n            <div class=\"mt-2 floating-label\">\n              <input\n                type=\"text\"\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                class=\"bg-[#FFE1A01F] h-[60px] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_placeholder or ''}}\"\n                {% if field.field_mandatory %} required {% endif %}\n              />\n              <label\n              for=\"{{ field.field_name }}\"\n              class=\"text-[#000000B2] text-sm font-normal\"\n              >{{field.field_label}}</label\n            >\n            </div>\n          </div>\n          {% endif %} {% if field.field_name == \"last_name\" %}\n          <div class=\"sm:col-span-3\">\n            <div class=\"mt-2 floating-label\">\n              <input\n                type=\"text\"\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                class=\"bg-[#FFE1A01F] h-[60px] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_placeholder or ''}}\"\n                {% if field.field_mandatory %} required {% endif %}\n              />\n              <label\n              for=\"{{ field.field_name }}\"\n              class=\"text-[#000000B2] text-sm font-normal\"\n              >{{field.field_label}}</label\n            >\n            </div>\n          </div>\n          {% endif %} \n          {% if (field.field_type == \"text\" or field.field_type == \"email\" or field.field_type == \"number\")\n          and field.field_name != \"first_name\" and field.field_name != \"last_name\"\n          %}\n          <div class=\"sm:col-span-6\">\n            <div class=\"mt-2 floating-label\">\n              <input\n                type=\"text\"\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                class=\"bg-[#FFE1A01F] h-[60px] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_placeholder or ''}}\"\n                {% if field.field_mandatory %} required {% endif %}\n              />\n              <label\n              for=\"{{ field.field_name }}\"\n              class=\"text-[#000000B2] text-sm font-normal\"\n              >{{field.field_label}}</label\n            >\n            </div>\n          </div>\n          {% endif %} {% if field.field_type == \"textarea\" %}\n          <div class=\"col-span-full\">\n            <div class=\"mt-2 floating-label\">\n              <textarea\n                name=\"{{ field.field_name }}\"\n                id=\"{{ field.field_name }}\"\n                rows=\"5\"\n                class=\"bg-[#FFE1A01F] block w-full rounded-md border-0 py-1.5 pl-[14px] text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6\"\n                placeholder=\"{{ field.field_placeholder or '' }}\"\n                {% if field.field_mandatory %} required {% endif %}\n              ></textarea>\n              <label\n                for=\"{{ field.field_name }}\"\n                class=\"label-textarea text-[#000000B2] text-sm font-normal\"\n              >{{field.field_label}}</label\n        >\n            </div>\n          </div>\n          {% endif %} {% endif %} {% endfor %}\n        </div>\n        <div class=\"flex justify-center\">\n          <button type=\"submit\" class=\"mt-4 mb-5\">\n            <div\n              class=\"px-4 py-3 text-white bg-[#6B090B] hover:bg-[#FFE1A0] hover:text-black rounded-lg border border-white hover:border-black cursor-pointer w-fit h-[40px] flex justify-center items-center\"\n            >\n              {{ form_data.btn_text }}\n            </div>\n          </button>\n        </div>\n      </form>\n      {% endif %}\n    </div>", "custom_css": ".floating-label {\n        position: relative;\n      }\n      .floating-label input:focus + label,\n      .floating-label input:not(:placeholder-shown) + label {\n        transform: translateY(-3em);\n        font-size: 0.75rem;\n        color: #4a5568; /* Tailwind's gray-700 */\n        background-color: white;\n      }\n\n      .floating-label textarea:focus + label,\n      .floating-label textarea:not(:placeholder-shown) + label {\n        transform: translateY(-2.2em);\n        font-size: 0.75rem;\n        color: #4a5568; /* Tailwind's gray-700 */\n        background-color: white;\n      }\n\n      .floating-label label {\n        position: absolute;\n        top: 50%;\n        transform: translateY(-50%);\n        left: 10px;\n        pointer-events: none;\n        transition: all 0.2s ease-out;\n        padding: 0 0.25em;\n      }\n\n      .floating-label .label-textarea {\n        top: 10%;\n        transform: translateY(-10%);\n      }", "custom_js": "    <script>\n      $(document).ready(function () {\n        $(\"#form-contact-btn1\").submit(function (event) {\n          event.preventDefault();\n          let formData = new FormData(this);\n          formData.append(\"utm_source\", getCookie(\"utm_source\") || \"\");\n          formData.append(\"utm_campaign\", getCookie(\"utm_campaign\") || \"\");\n          formData.append(\n            \"source\",\n            getCookie(\"source\") || window.location.href\n          );\n\n          $.ajax({\n            type: \"POST\",\n            url: \"{{form_data.form_action}}\",\n            data: formData,\n            processData: false,\n            contentType: false,\n            headers: {\n              \"X-Frappe-CSRF-Token\": \"{{csrf_token}}\"\n            },\n            success: function (response) {\n              Swal.fire({\n                position: \"bottom-end\",\n                icon: \"success\",\n                title: \"<h4>Gửi thông tin thành công</h4>\",\n                showConfirmButton: false,\n                timer: 1500\n              });\n              $(\"#form-contact-btn1\")[0].reset();\n            },\n            error: function (error) {\n              Swal.fire({\n                position: \"bottom-end\",\n                icon: \"error\",\n                title: \"<h4>Gửi thông tin thất bại</h4>\",\n                showConfirmButton: false,\n                timer: 1500\n              });\n            }\n          });\n        });\n      });\n    </script>", "form": "0e0cf2671e", "doctype": "Page Section", "content": [{"name": "eabcdd6e01", "owner": "Administrator", "creation": "2024-10-12 10:13:47.256822", "modified": "2024-10-12 10:13:59.583485", "modified_by": "Administrator", "docstatus": 0, "idx": 1, "field_label": "Tiêu đề", "field_key": "title", "field_type": "Text", "content_type": "Data", "allow_update_to_style": 0, "content": "Liên hệ", "parent": "PS-24-0000014210", "parentfield": "content", "parenttype": "Page Section", "doctype": "Section Content"}, {"name": "d530c6679a", "owner": "Administrator", "creation": "2024-10-12 10:13:47.256822", "modified": "2024-10-12 10:13:59.583485", "modified_by": "Administrator", "docstatus": 0, "idx": 2, "field_label": "Mô tả", "field_key": "descriptions", "field_type": "List", "content_type": "Data", "allow_update_to_style": 0, "content": "[{\"describe\": \"\\u0110\\u1ec3 l\\u1ea1i th\\u00f4ng tin li\\u00ean h\\u1ec7 ngay v\\u1edbi ch\\u00fang t\\u00f4i\", \"idx\": 1}]", "